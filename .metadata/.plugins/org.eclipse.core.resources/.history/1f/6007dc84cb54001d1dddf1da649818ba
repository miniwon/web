$(function() {

	var topDiv = $(".tabSet");
	var anchors = topDiv.find("ul.tabs > li > a");
	var panelDivs = topDiv.find("div.panels > div.panel");
	
	anchors.show();
	panelDivs.hide();
	
	// filter: 속성의 값까지 찾을 때
	var lastAnchor = anchors.filter(".on");
	var lastPanel = $(lastAnchor.attr("href"));
	alert(lastPanel);
	
	

});